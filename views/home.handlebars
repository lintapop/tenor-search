<!-- home.handlebars -->
<h1>Search for GIFS</h1>
<form action="/">
    <input type="text" name="term">
    <button type="submit">Search</button>
</form>


{{#each gifs}}
<div class="tenor-gif-embed" data-postid="{{this.id}}" data-share-method="host" data-width="300px"
    data-aspect-ratio="1.2266666666666666"><a href="{{this.itemurl}}"></a>
    <a href="{{this.url}}"></a>
</div>
<script type="text/javascript" async src="https://tenor.com/embed.js"></script>
{{/each}}